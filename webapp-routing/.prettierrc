{
	"jsxSingleQuote": true,
	"trailingComma": "all",
	"tabWidth": 2,
	"bracketSameLine": false,
	"arrowParens": "always",
	"semi": false,
	"singleQuote": true,
	"htmlWhitespaceSensitivity": "ignore",
	"useTabs": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
